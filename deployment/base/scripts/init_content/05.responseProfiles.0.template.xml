<?xml version="1.0" encoding="UTF-8"?>
<xml>
	<config>
		<serviceUrl>@SERVICE_URL@</serviceUrl>
		<partnerId>-2</partnerId>
		<clientTag>Stand alone php 1.0.0</clientTag>
		<curlTimeout>120</curlTimeout>
		<userAgent>Stand alone php 1.0.0</userAgent>
		<verifySSL objectType="bool">0</verifySSL>
	</config>
	<session>
		<partnerId>-2</partnerId>
		<secret>@ADMIN_CONSOLE_PARTNER_ADMIN_SECRET@</secret>
		<userId></userId>
		<sessionType>2</sessionType>
		<expiry>86400</expiry>
		<privileges></privileges> 
	</session>

	<multirequest>
		<request service="responseProfile" action="add" partnerId="0">
			<responseProfile objectType="KalturaResponseProfile">
				<type>1</type>
				<name>pollVoteResponseProfile</name>
				<systemName>pollVoteResponseProfile</systemName>
				<fields>id,name,createdAt,updatedAt,parentId,partnerData,text,tags,userId,startTime,endTime</fields>
				<relatedProfiles objectType="array">
					<item objectType="KalturaDetachedResponseProfile">
						<name>pollVoteResponseProfile</name>
						<type>1</type>
						<fields>id,objectId,createdAt,xml</fields>
						<filter objectType="KalturaMetadataFilter">
							<metadataObjectTypeEqual>annotationMetadata.Annotation</metadataObjectTypeEqual>
						</filter>
						<mappings objectType="array">
							<item objectType="KalturaResponseProfileMapping">
								<parentProperty>id</parentProperty>
								<filterProperty>objectIdEqual</filterProperty>
							</item>
						</mappings>
					</item>
				</relatedProfiles>
			</responseProfile>
		</request>
	</multirequest>

	<multirequest>
		<request service="responseProfile" action="add" partnerId="0">
			<responseProfile objectType="KalturaResponseProfile">
				<type>1</type>
				<name>QandA</name>
				<systemName>QandA</systemName>
				<fields>id,name,createdAt,updatedAt,parentId,partnerData,text,tags,userId,startTime,endTime</fields>
				<relatedProfiles objectType="array">
					<item objectType="KalturaDetachedResponseProfile">
						<name>QandA_ResponseProfile</name>
						<type>1</type>
						<fields>id,objectId,createdAt,xml</fields>
						<filter objectType="KalturaMetadataFilter">
							<metadataObjectTypeEqual>annotationMetadata.Annotation</metadataObjectTypeEqual>
						</filter>
						<mappings objectType="array">
							<item objectType="KalturaResponseProfileMapping">
								<parentProperty>id</parentProperty>
								<filterProperty>objectIdEqual</filterProperty>
							</item>
						</mappings>
					</item>
				</relatedProfiles>
			</responseProfile>
		</request>
	</multirequest>

	<multirequest>
		<request service="responseProfile" action="add" partnerId="0">
			<responseProfile objectType="KalturaResponseProfile">
				<type>1</type>
				<name>reach_vendor</name>
				<systemName>reach_vendor</systemName>
				<fields>id,partnerId,vendorPartnerId,createdAt,updatedAt,queuedAt,finishedAt,entryId,status,vendorProfileId,catalogItemId,price,userId,approvedBy,errDescription,accessKey,notes</fields>
				<relatedProfiles objectType="array">
					<item objectType="KalturaDetachedResponseProfile">
						<name>reach_vendor_catalog_item</name>
						<type>2</type>
						<fields>id,vendorPartnerId,name,systemName,createdAt,updatedAt,status,pricing,fixedPriceAddons</fields>
						<filter objectType="KalturaVendorCatalogItemFilter">
						</filter>
						<mappings objectType="array">
							<item objectType="KalturaResponseProfileMapping">
								<parentProperty>catalogItemId</parentProperty>
								<filterProperty>idEqual</filterProperty>
							</item>
						</mappings>
					</item>
					<item objectType="KalturaDetachedResponseProfile">
						<name>reach_vendor_profile</name>
						<type>2</type>
						<fields>autoDisplayMachineCaptionsOnPlayer,autoDisplayHumanCaptionsOnPlayer,enableMetadataExtraction,enableSpeakerChangeIndication,enableAudioTags,enableProfanityRemoval,maxCharactersPerCaptionLine</fields>
						<filter objectType="KalturaVendorProfileFilter">
						</filter>
						<mappings objectType="array">
							<item objectType="KalturaResponseProfileMapping">
								<parentProperty>vendorProfileId</parentProperty>
								<filterProperty>idEqual</filterProperty>
							</item>
						</mappings>
					</item>
				</relatedProfiles>
			</responseProfile>
		</request>
	</multirequest>
	
</xml>
