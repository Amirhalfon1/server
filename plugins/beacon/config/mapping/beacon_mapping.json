{
	"settings" : {
		"number_of_shards" : 6,
		"number_of_replicas" : 0,
		"analysis": {
			"char_filter": {
				"kaltura_whitespace_filter": {
					"type": "pattern_replace",
					"pattern": "(\\s+)",
					"replacement": " "
				}
			},
			"analyzer": {
				"kaltura_keyword": {
					"type": "custom",
					"tokenizer": "keyword",
					"char_filter": [
						"kaltura_whitespace_filter"
					],
					"filter": [
						"lowercase",
						"trim"
					]
				}
			}
		}
	},
	"mappings": {
		"State": {
			"dynamic_templates": [
				{
					"integers": {
						"match_mapping_type": "long",
						"mapping": {
							"type": "float"
						}
					}
				}
			],
			"properties": {
				"createdAt": {
					"type": "date",
					"format": "epoch_second"
				},
				"eventType": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"objectId": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"partnerId": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"privateData": {
					"type" : "object",
				  	"properties": {
					  "timestamp": {"type":"date"}
					}
				},
				"rawData": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"relatedObjectType": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"updatedAt": {
					"type": "date",
					"format": "epoch_second"
				}
			}
		},
		"Log": {
			"dynamic_templates": [
				{
					"integers": {
						"match_mapping_type": "long",
						"mapping": {
							"type": "float"
						}
					}
				}
			],
			"properties": {
				"createdAt": {
					"type": "date",
					"format": "epoch_second"
				},
				"eventType": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"objectId": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"partnerId": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"privateData": {
					"type" : "object"
				},
				"rawData": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"relatedObjectType": {
					"type" : "text",
					"analyzer" : "kaltura_keyword"
				},
				"updatedAt": {
					"type": "date",
					"format": "epoch_second"
				}
			}
		}
	}
}
